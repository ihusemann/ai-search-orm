{
  "name": "ivy-kit",
  "version": "0.0.3",
  "description": "",
  "scripts": {
    "cli": "tsx ./src/index.ts",
    "build": "rm -rf ./dist && tsx build.ts && cp package.json dist/",
    "build:dev": "rm -rf ./dist && tsx build.dev.ts && chmod +x ./dist/index.cjs",
    "myPack": "cp package.json README.md dist/ && (cd dist && npm pack --pack-destination ..) && ls && rm -f package.tgz && mv *.tgz package.tgz",
    "myPublish": "npm publish package.tgz",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "ivy-kit": "./bin.cjs"
  },
  "keywords": [
    "ivy",
    "orm",
    "azure",
    "ai",
    "search",
    "ivy-kit",
    "schema",
    "migrations",
    "typescript",
    "ts",
    "cognitive"
  ],
  "author": "ihusemann",
  "license": "MIT",
  "dependencies": {
    "@drizzle-team/brocli": "^0.10.1",
    "@esbuild-kit/esm-loader": "^2.6.5",
    "esbuild": "^0.23.1",
    "esbuild-register": "^3.6.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "@azure/identity": "^4.1.0",
    "@azure/search-documents": "^12.0.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^20.15.0",
    "@types/pluralize": "^0.0.33",
    "@types/prompts": "^2.4.9",
    "ivy-orm": "workspace:*",
    "chalk": "^5.3.0",
    "cosmiconfig": "^9.0.0",
    "ora": "^8.0.1",
    "pluralize": "^8.0.0",
    "prompts": "^2.4.2",
    "tsup": "^8.0.2",
    "tsx": "^4.16.2",
    "zod": "^3.23.8",
    "zx": "^8.1.4"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.mts",
        "default": "./index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    }
  }
}
